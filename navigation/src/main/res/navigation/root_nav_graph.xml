<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root_nav_graph"
    app:startDestination="@id/blankFragment">
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.dashkevich.navigation.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" >
        <action
            android:id="@+id/action_blankFragment_to_bottom"
            app:destination="@id/bottom"
            app:launchSingleTop="true"
            app:popUpTo="@id/blankFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <include app:graph="@navigation/bottom" />
</navigation>